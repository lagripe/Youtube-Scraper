{
  "manifest_version": 2,

  "name": "Youtube email scrapper",
  "description": "An extension that helps you scrap email from youtube (about)",
  "version": "1.0",

  "icons": {
    "48": "icon.png"
  },

  "permissions": [
    "tabs", "<all_urls>"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}